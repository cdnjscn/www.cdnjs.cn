<style>
.list {
	margin:20px 0;
}

.project {
	font-size:14px;
	margin:18px 0;
	border:solid 1px #dedede;
	padding:10px;
	background-color:#f9f9f9;
}


.collection-head {
	background-image: url(data:image/svg+xml;base64,);
}

.collection-header .ver {
	font-size:18px;
}

.project-plus {	
	padding:10px 0;
	border-bottom:solid 1px #eee;
}
.project-plus a{
	color:#333;
}

.btm-focus {
	height: 60px;
	margin-bottom: 20px;
	text-align: center;
}
</style>

<div class="main">
	<div class="breadcrum">
		<a href="/">首页</a> > {{project.name}}
	</div>
</div>

<div class="collection-head">
	<div class="container">
		<div class="collection-title">
			<h1 class="collection-header">
				{{project.name}} <span class="ver">({{project.version}})</span>
			</h1>
			<div class="collection-info">
				<p>{{project.description}}</p>
			</div>
		</div>
	</div>
</div>

<div class="main">
	<div class="project-plus">
		<a target="_blank" rel="nofollow" href="{{project.homepage}}">项目网站</a>{{#project.repositories}} / <a target="_blank" rel="nofollow" href="{{url}}">源码</a>{{/project.repositories}}{{#project.bugs}} / <a target="_blank" rel="nofollow" href="{{project.bugs}}">问题反馈</a>{{/project.bugs}}
	</div>
	<div class="list">
{{#project.assets}}
<div class="item-wrapper">
<h4><strong>{{version}}</strong></h4>
<ul class="item">
{{#files}}
<li>{{site}}/libs/{{project.name}}/{{version}}/{{.}}</li>
{{/files}}
</ul>
{{#hasExt}}
<a href="#" class="ext">展开</a>
{{/hasExt}}
</div>
{{/project.assets}}
</div>

<div class="btm-focus">
	<a href="http://www.ucloud.cn/act/index/3rdanniver_sale_01?sem=3rd-1-cdnjs" target="_blank"><img src="http://cdnjscn.b0.upaiyun.com/plus/cdnjs.cn/imgs/20150227.jpg" height="60" width="640"></a>
</div>
</div>